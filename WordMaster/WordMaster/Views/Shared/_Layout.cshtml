<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"]Podnieś umiejętności językowe</title>
    <link rel="stylesheet" href="~/css/globalSite.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/loading.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/messageContainer.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/topPanelContainer.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/wordsListContainer.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/bottomPanelContainer.css" asp-append-version="true"/>
</head>
<body>
    <header></header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>
    <footer></footer>
    <script src="~/js/timer.js" asp-append-version="true"></script>
    <script src="~/js/validation.js" asp-append-version="true"></script>
    <script src="~/js/word.js" asp-append-version="true"></script>
    <script src="~/js/buttons.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
